oncall("executorch")

cpp_unittest(
    name = "pytree_test",
    srcs = ["test_pytree.cpp"],
    deps = ["//executorch/extension/pytree:pytree"],
)

cpp_unittest(
    name = "function_ref_test",
    srcs = ["function_ref_test.cpp"],
    deps = ["//executorch/extension/pytree:pytree"],
)

python_unittest(
    name = "pybindings_test",
    srcs = [
        "test.py",
    ],
    deps = [
        "//caffe2:torch",
        "//executorch/extension/pytree:pybindings",
