# @noautodeps
load("@fbcode_macros//build_defs:cpp_unittest.bzl", "cpp_unittest")
load("@fbcode_macros//build_defs:python_unittest.bzl", "python_unittest")

oncall("executorch")

cpp_unittest(
    name = "cpptest",
    srcs = ["test_pytree.cpp"],
    supports_static_listing = True,
    deps = ["//executorch/pytree:pytree"],
)

python_unittest(
    name = "test",
    srcs = [
        "test.py",
    ],
    supports_static_listing = True,
    deps = [
        "//caffe2:torch",
        "//executorch/pytree:pybindings",
        "//executorch/pytree:pylib",
    ],
)
