    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug CMake project",
            "type": "lldb", // https://github.com/vadimcn/vscode-lldb
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": [
                "--model_path=./add.pte",
            ]
        },
        {
            "name": "Debug python proj",
            "type": "debugpy",
            "request": "launch",
            "module": "unittest",
            "args": [
                "./backends/xnnpack/test/passes/test_channels_last_tagged_reshape.py"
            ]
        },
    ]
}
