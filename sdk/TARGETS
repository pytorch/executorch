oncall("executorch")

python_library(
    name = "lib",
    srcs = [
        "__init__.py",
        "lib.py",
    ],
    deps = [
        "//executorch/sdk/edir:et_schema",
        "//executorch/sdk/etdb:etdb",
        "//executorch/sdk/etdb:inspector",
        "//executorch/sdk/etrecord:etrecord",
    ],
)

python_binary(
    name = "cli",
    main_src = "lib.py",
    par_style = "xar",
    deps = [
        "//executorch/sdk/edir:et_schema",
        "//executorch/sdk/etdb:etdb",
        "//executorch/sdk/etdb:inspector",
        "//executorch/sdk/etrecord:etrecord",
    ],
